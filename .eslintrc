{
  "env": {
    "vue/setup-compiler-macros": true
  },
  "plugins": ["prettier"],
  "extends": [
    "@nuxtjs/eslint-config-typescript",
    "plugin:nuxt/recommended",
    "plugin:@peeky/recommended",
    "prettier"
  ],
  "rules": {
    "vue/multi-word-component-names": [
      "error",
      {
        "ignores": ["Header", "Footer", "Logo"]
      }
    ]
  }
}
